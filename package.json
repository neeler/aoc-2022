{
  "name": "@neeler/aoc-2022",
  "version": "1.0.0",
  "description": "Solutions for Advent of Code 2022",
  "scripts": {
    "generate": "node -r @swc-node/register src/scripts/generate.ts",
    "dev": "nodemon ./src/index.ts",
    "prebuild": "npm run clean",
    "build": "swc src -d dist",
    "copy:data": "npx copyfiles -u 1 src/data/**/* dist",
    "clean": "npx rimraf dist",
    "prestart": "npm run build && npm run copy:data",
    "start": "node ./dist/index.js",
    "lint:es": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "neeler",
  "license": "ISC",
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "8.19.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "^18.11.10",
    "copyfiles": "^2.4.1",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-only-warn": "^1.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
